{
   "config": {
      "hawtio": {
         "authenticationEnabled": false,
         "context": "/hawtio",
         "port": 8000.0,
         "start": false,
         "war": "./hawtio/hawtio-default-offline-1.4.48.war"
      },
      "server": {
         "apps": {
            "install": {
               "allow": true
            },
            "upgrade": {
               "allow": true
            }
         },
         "clock": {
            "source": "system"
         },
         "demo": {
            "file": "./src/test/resources/motu.log&scanStream=true&scanStreamDelay=500",
            "start": false
         },
         "files": {
            "maps": {
               "dir": "./mapcache"
            },
            "static": {
               "dir": "./signalk-static"
            },
            "storage": {
               "root": "./storage/"
            },
            "usb": {
               "usbdrive": "/media/usb0"
            }
         },
         "serial": {
            "baud": 38400.0,
            "enable": true,
            "ports": ["/dev/ttyUSB0","/dev/ttyUSB1","/dev/ttyUSB2","/dev/ttyACM0","/dev/ttyACM1","/dev/ttyACM2"]
         },
         "server": {
            "mqtt": {
               "port": 1883.0,
               "start": true
            },
            "nmea": {
               "generate0183": true
            },
            "rest": {
               "port": 8080.0
            },
            "stomp": {
               "port": 61613.0,
               "start": true
            },
            "tcp": {
               "nmea": {
                  "port": 55557.0
               },
               "port": 55555.0
            },
            "udp": {
               "nmea": {
                  "port": 55556.0
               },
               "port": 55554.0
            },
            "websocket": {
               "port": 3000.0
            }
         },
         "version": "v1.0.0",
         "vessel": {
            "uuid": "motu"
         }
      }
   }
}
